/* pages/keich/keich.wxss */
.header{
  height: 150rpx;
  background-color: #BCE7FD;
  display: block;
}
.cost{
  border: 1rpx solid #000;
  width: 80%;
  height: 50rpx;
  text-align: center;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  margin-top: 40rpx;
  border-radius: 10rpx;
}
.cost:after{
  content: ""; 
  clear: both;
}
.tabBorer{
  color: #FC8585;
}
.header .header_left{
  height: 50rpx;
  width: 50%;
  display: inline-block;
  text-align: center;
  line-height: 50rpx;
}
.header .header_right{
  height: 50rpx;
  width: 50%;
  display: inline-block;
  line-height: 50rpx;
}